PVMTestSuite DEFINITIONS ::= BEGIN

Testcase ::= SEQUENCE {
    name UTF8String,
    initial-regs SEQUENCE (SIZE(13..13)) OF INTEGER(0..4294967295),
    code SEQUENCE OF INTEGER(0..255),
    expected-status ENUMERATED {
        trap,
        halt
    },
    expected-regs SEQUENCE (SIZE(13..13)) OF INTEGER(0..4294967295)
}

END
